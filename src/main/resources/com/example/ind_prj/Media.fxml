<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@my.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ind_prj.AudioController">
    <children>
        <Button layoutX="47.0" layoutY="207.0" mnemonicParsing="false" onAction="#play" prefHeight="42.0" prefWidth="70.0" styleClass="player-btn" stylesheets="@my.css" text="PLAY" />
        <Button layoutX="139.0" layoutY="207.0" mnemonicParsing="false" onAction="#pause" prefHeight="42.0" prefWidth="70.0" styleClass="player-btn" stylesheets="@my.css" text="PAUSE" />
        <Button layoutX="229.0" layoutY="207.0" mnemonicParsing="false" onAction="#next" prefHeight="42.0" prefWidth="70.0" styleClass="player-btn" stylesheets="@my.css" text="NEXT" />
        <Button layoutX="317.0" layoutY="207.0" mnemonicParsing="false" onAction="#previous" prefHeight="42.0" prefWidth="70.0" styleClass="player-btn" stylesheets="@my.css" text="PREVIOUS" />
        <Label fx:id="label_audio" alignment="CENTER" layoutY="59.0" prefHeight="74.0" prefWidth="600.0" styleClass="player-title" stylesheets="@my.css" text="AUDIO PLAYER">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Slider fx:id="volume" layoutX="411.0" layoutY="221.0" stylesheets="@my.css" />
        <ProgressBar fx:id="progressBar" layoutX="17.0" layoutY="148.0" prefHeight="42.0" prefWidth="563.0" progress="0.0" stylesheets="@my.css" />
      <Button fx:id="btnBack" mnemonicParsing="false" onAction="#handleBack" prefHeight="25.0" prefWidth="59.0" styleClass="player-back-btn" stylesheets="@my.css" text="Back" />
    </children>
</AnchorPane>
